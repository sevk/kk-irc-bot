#!/bin/bash -e

echo run kk-irc-bot and proxy in screen

#git clone --depth 1 git://github.com/phus/goagent.git
sudo -u kk -i bash -c "cd /home/kk/dev/goagent/local ; screen -dm python ./proxy.py "
sudo -u kk -i bash -c "cd /home/kk/dev/kk-irc-bot ; screen -dm ./irc.rb ub.conf & "
#sudo -ukk -i bash -c "cd /home/kk/dev/kk-irc-bot;screen -d -m ./irc.rb avplayer.conf &"


